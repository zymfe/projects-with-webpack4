<style type="text/css">
  .container {
    font-size: 30px; 
    background-color: blue; 
    color: red;
  }
</style>

<div class="container">
  <div class="box">
    <div>名字：<%=name %></div>
    <div>年龄：<%=age %></div>
    <div>
      技能：
      <% for (var i = 0; i < skill.length; i += 1){ %>
        <%= skill[i] %>
      <% } %>
    </div>
  </div>
</div>
